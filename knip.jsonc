{
  "$schema": "https://unpkg.com/knip@5/schema-jsonc.json",
  "paths": {
    "src/*": ["./src/*"],
  },
  "project": ["src/**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"],
  "ignoreExportsUsedInFile": true,
  "ignoreDependencies": [
    // ignore dependencies
    "@fontsource.+",
  ],
  "ignore": [
    // ignore folders or files
    "src/_mock/**",
  ],
}
